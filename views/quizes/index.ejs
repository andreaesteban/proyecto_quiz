<form method="get" action="/quizes?search">
  Buscar: 
    <input type="text" name="search" placeholder="Buscar por preguntas con..." />
    <input type="submit" value="Enviar" />
</form>

<table class="sortable">
  <% var i;
    
    for (i=0; i<quizes.length; i++) { %>
      <tr><td><a href="quizes/<%= quizes[i].id %>">
          <%= quizes[i].pregunta %>
          </a>
      </td></tr>
  <% } %>
</table>

